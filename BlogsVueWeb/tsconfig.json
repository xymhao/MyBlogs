{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": [ "webpack-env" ],
    "typeRoots": [
      "./types",
      "./node_modules/vue/types"
    ]
  },
  "exclude": [
    "bin",
    "node_modules"
  ]
}
